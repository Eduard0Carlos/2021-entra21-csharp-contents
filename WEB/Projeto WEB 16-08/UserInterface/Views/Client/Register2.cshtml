@{
    Layout = null;
}
<html>

<head>
    <title>Pagina de cadastro</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
</head>

<body>
    <div class="row">
        <div class="col-md-4">
            <form>
                <div class="form-group">
                    <label class="custom-control-label">Nome</label>
                    <input class="form-control" type="text" name="Nome" id="Nome" />
                </div>

                <div class="form-group">
                    <label class="custom-control-label">CPF</label>
                    <input class="form-control" type="text" name="CPF" id="CPF" />
                </div>

                <div class="form-group">
                    <label class="custom-control-label">RG</label>
                    <input class="form-control" type="text" name="RG" id="RG" />
                </div>

                <div class="form-group">
                    <label class="custom-control-label">Email</label>
                    <input class="form-control" type="text" name="Email" id="Email" />
                </div>

                <div class="form-group">
                    <label class="custom-control-label">Data de Nascimento</label>
                    <input class="form-control" type="datetime" name="DataNascimento" id="DataNascimento" />
                </div>
            </form>
        </div>
    </div>
</body>
</html>